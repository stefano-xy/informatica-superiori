                        </then>
                        <else>
                            <if expression="n1 = n2 &amp;&amp; n2 = n3">
                                <then>
                                    <output expression="&quot;Numeri uguali&quot;" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot;Nessun ordine particolare&quot;" newline="True"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
