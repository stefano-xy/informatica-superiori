    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="n1, n2, n3" type="Integer" array="False" size=""/>
            <output expression="&quot;Inserire 3 numeri&quot;" newline="True"/>
            <input variable="n1"/>
            <input variable="n2"/>
            <input variable="n3"/>
            <if expression="n1 &lt; n2 &amp;&amp; n2 &lt; n3">
                <then>
                    <output expression="&quot;Ordine crescente&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="n1 &gt; n2 &amp;&amp; n2 &gt; n3">
                        <then>
